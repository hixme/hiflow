{
  "name": "hiflow",
  "version": "0.3.4",
  "description": "A pull request and gitflow management tool",
  "main": "lib/index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "bin": {
    "hi": "bin/index.js",
    "hiflow": "bin/index.js"
  },
  "files": [
    "src",
    "bin"
  ],
  "keywords": [
    "gitflow",
    "jira",
    "bitbucket"
  ],
  "author": "Billy Montgomery billy.montgomery@gmail.com",
  "dependencies": {
    "axios": "^0.19.0",
    "babel-runtime": "^6.23.0",
    "chalk": "^2.3.0",
    "inquirer": "^6.0.0",
    "inquirer-chalk-pipe": "^1.2.0",
    "js-yaml": "^3.9.0",
    "ora": "^3.0.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.0.0",
    "babel-plugin-add-module-exports": "^0.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.5",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.73.0",
    "jest": "^22.0.4",
    "jest-cli": "^23.0.0",
    "mocha": "^5.2.0",
    "prettier": "^1.14.2",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:examples": "node examples/",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- --compilers js:babel-register --recursive",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir bin --source-maps",
    "flow": "flow",
    "prepublish": "npm run clean && npm run build && npm run test"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hixme/hiflow.git"
  },
  "license": "MIT"
}
